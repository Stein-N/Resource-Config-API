{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "Your name here": "${mod_author}"
      },
      "contact": {
        "homepage": "https://github.com/Stein-N/Resource-Config-API",
        "sources": "https://github.com/Stein-N/Resource-Config-API",
        "issues": "https://github.com/Stein-N/Resource-Config-API/issues"
      },
      "icon": "logo.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "net.xstopho.resource_config_api.ResourceConfig"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.19.1"
      },
      {
        "id": "minecraft",
        "versions": ">=1.20 <=1.21"
      }
    ]
  }
}