{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [ "${mod_author}" ],
    "contact": {
        "homepage": "https://github.com/Stein-N/Resource-Config-API",
        "sources": "https://github.com/Stein-N/Resource-Config-API",
        "issues": "https://github.com/Stein-N/Resource-Config-API/issues"
    },
    "license": "${mod_license}",
    "icon": "icon_fabric.png",
    "environment": "*",
    "entrypoints": {
        "main": [ "net.xstopho.resource_config_api.ResourceConfig" ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": "*",
        "minecraft": ">=1.21 <1.22",
        "java": ">=21"
    },
    "custom": {
        "modmenu": {
            "badges": ["library"]
        }
    }
}